{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "vmUsername": {
            "value": "ENTERUSERNAME"
        },
        "vmPassword": {
            "value": ${{ secrets.VM_PW }}
        },
        "accountName": {
            "value": ${{ secrets.ACCOUNT_NAME }}
        },
        "personalAccessToken": {
            "value": ${{ secrets.PAT }}
        },
        "workloadName" :{ 
            "value": "wlname"
        },
        "environment" :{ 
            "value": "dev"
        },
        "CICDAgentType": {
            "value": "azuredevops OR github OR none"
        }

    }
}
